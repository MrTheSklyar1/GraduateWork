    <v:String x:Key="m_CardNotFound">Card not found</v:String>
    <v:String x:Key="m_UserNotFound">Personal role not found</v:String>
    <v:String x:Key="m_RoleNotFound">Role not found</v:String>
    <v:String x:Key="m_TypeNotFound">Document type not found</v:String>
    <v:String x:Key="m_CardNotCreated">Card not created</v:String>
    <v:String x:Key="m_CardViewNotCreated">Card view not created</v:String>
    <v:String x:Key="m_StateNotFound">State not found</v:String>
