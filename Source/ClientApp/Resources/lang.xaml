    <v:String x:Key="m_tab_LogIn">Log In</v:String>
    <v:String x:Key="m_tab_LogIn_LogInText">Use your Login and Password to access the system</v:String>
    <v:String x:Key="m_tab_LogIn_LogIn">Login</v:String>
    <v:String x:Key="m_tab_LogIn_PassWord">Password</v:String>
